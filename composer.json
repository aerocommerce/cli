{
    "name": "aerocommerce/cli",
    "description": "Aero Commerce CLI Tool.",
    "keywords": [
        "aero",
        "ecommerce"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Aero\\Cli\\": "src/"
        }
    },
    "require": {
        "php": "^7.2.9",
        "ext-json": "*",
        "illuminate/support": "^5.8|^6.0|^7.0",
        "symfony/console": "^4.2|^5.0",
        "symfony/process": "^4.2|^5.0",
        "symfony/http-client": "^4.2|^5.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "aero"
    ]
}
