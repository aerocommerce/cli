{
  "name": "aerocommerce/cli",
  "description": "Aero Commerce CLI Tool.",
  "keywords": [
    "aero",
    "ecommerce"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tim Spratt",
      "email": "tim.spratt@techquity.co.uk"
    }
  ],
  "autoload": {
    "psr-4": {
      "Aero\\Cli\\": "src/"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "require": {
    "php": "^7.1.3",
    "illuminate/support": "~5.1|~5.2|~5.3|~5.4|~5.5|~5.6|~5.7",
    "symfony/console": "~2.7|~3.0|~4.0",
    "symfony/process": "~2.7|~3.0|~4.0",
    "guzzlehttp/guzzle": "~6.0",
    "ext-json": "^1.5",
    "ext-posix": "^7.1"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": [
    "aero"
  ]
}
